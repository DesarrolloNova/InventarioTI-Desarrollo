@model IEnumerable<InventarioTI.Models.Licencias>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Licencias</h1>

<p>
    <a class="btn btn-outline-primary" asp-action="Create">Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Marca)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tipo)
            </th>
            @* <th>
            @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th>
            @Html.DisplayNameFor(model => model.Clave)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.CodigoProducto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InicioVigencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FinVigencia)
            </th>
            <th>
                <a>Acciones</a>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Marca)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tipo)
                </td>
                @*<td>
            @Html.DisplayFor(modelItem => item.Usuario)
            </td>
            <td>
            @Html.DisplayFor(modelItem => item.Clave)
            </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.CodigoProducto)
                </td>
                <td>
                    @*@Html.DisplayFor(modelItem => item.InicioVigencia)*@
                    <a>@item.InicioVigencia.ToString("dd/MM/yyyy")</a>
                </td>
                <td>
                    @*@Html.DisplayFor(modelItem => item.FinVigencia)*@
                    <a>@item.FinVigencia.ToString("dd/MM/yyyy")</a>
                </td>
                <td>
                    <a class="btn btn-primary" asp-action="Details" asp-route-id="@item.Id">Detalles</a>
                    <a class="btn btn-success" asp-action="Edit" asp-route-id="@item.Id">Editar</a>
                    @*<a asp-action="Delete" asp-route-id="@item.Id">Delete</a>*@
                </td>
            </tr>
        }
    </tbody>
</table>
